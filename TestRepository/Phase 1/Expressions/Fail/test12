bexp be,ae,pe,qe,re,se,we;
int a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p;
bit a,b,c;
be=a and (b or c) nand (c and a);
ae=(not a) and b;

a=2+3;
b=2-3;
c=2*3;
d=2/3;
e=(2%3);
f=2+3+4;
g=2+3-4;
h=2*(3/4);
i=2*3*4;
j=(2/3)/4;
k=2%3%4;
l=2%3/4;
m=2*(3)%4;
n=2+3/4;
o=(2+3)-(4*(5%6));
p=2+(3-4*5/(6%7));

pe=(a) or b;
qe=a and (b or c);
re=a and not b;
se=(not a) and b;
we=not ( a xnor b);
