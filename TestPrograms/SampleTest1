bexp be;
bit A,B,C; 
//be = A nand (B or not C) nor B xor C; 
be = A and C;
kmap k_map[2][2];
int rsize;
int csize;
rsize=rowSize(k_map);
csize=columnSize(k_map);
output(rsize);
output(csize);
int i;
int j;
while(i<rsize){
  
  j=0;
  
  while(j<csize){
   bit b;
   b=input();  
   k_map[i][j]=b;   

   j=j+1;
  }
  i=i+1;
}


//COMPARING THE TWO KMAPS
bit b;
b=compare(k_map,be);
output("b"+b);
if(b==T) {
output("Your Kmap is correct");
}
else {
output("Your Kmap is incorrect");
}
